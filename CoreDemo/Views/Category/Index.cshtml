@using EntityLayer.Concrete
@model List<Category>

@{
    ViewData["Title"] = "Index";
}

<table>
    <tr>
        <th>ID</th>
        <th>Kategroy Adı</th>
        <th>Kategroy Durumu</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryId</td>
             <td>@item.CategroyName</td>
             <td>@item.CategoryStatus</td>
        </tr>
    }
</table>

